    <UserSecretsId>0952bec6-20aa-4dfc-84c9-f701c5d396bc</UserSecretsId>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="BCrypt.Net-Next" Version="4.0.3" />
